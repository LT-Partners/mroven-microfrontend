{
  "name": "micro-frontend-template",
  "version": "1.0.0",
  "description": "Template for creating new micro frontends",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env NODE_ENV=local",
    "start:dev": "webpack serve --env NODE_ENV=development",
    "start:staging": "webpack serve --env NODE_ENV=staging",
    "start:prod": "webpack serve --env NODE_ENV=production",
    "build": "webpack --mode production --env NODE_ENV=production",
    "build:dev": "webpack --mode development --env NODE_ENV=development",
    "build:staging": "webpack --mode production --env NODE_ENV=staging",
    "preview": "serve dist -p 8081 --no-request-logging",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "test:coverage": "npx jest --coverage",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug"
  },
